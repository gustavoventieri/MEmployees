{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "gustavo ventieri",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "ts-node-dev ./src/index.ts",
    "test": "jest",
    "production": "node ./build/index.js",
    "preproduction": "tsc",
    "rollback": "knex --knexfile ./src/server/database/knex/Enviroment.ts migrate:rollback",
    "migrate": "knex --knexfile ./src/server/database/knex/Enviroment.ts migrate:latest",
    "rollback-all": "knex --knexfile ./src/server/database/knex/Enviroment.ts migrate:rollback --all",
    "seed": "knex --knexfile ./src/server/database/knex/Enviroment.ts seed:run"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "http-status-codes": "^2.3.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.2.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/supertest": "^6.0.2",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
